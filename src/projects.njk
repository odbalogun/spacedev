<script>
(function () {
  var filtersEl = document.getElementById('projects-filters');
  var gridEl = document.getElementById('projects-grid');
  if (!filtersEl || !gridEl) return;
  var buttons = filtersEl.querySelectorAll('.project-filter-btn');
  var items = gridEl.querySelectorAll('.masonry-item');
  function setActive(btn) {
    buttons.forEach(function (b) {
      b.setAttribute('aria-pressed', b === btn ? 'true' : 'false');
      b.classList.remove('bg-primary', 'text-background-dark');
      b.classList.add('bg-white/5', 'text-white');
    });
    btn.setAttribute('aria-pressed', 'true');
    btn.classList.add('bg-primary', 'text-background-dark');
    btn.classList.remove('bg-white/5', 'text-white');
  }
  function filterProjects(value) {
    items.forEach(function (item) {
      var cat = item.getAttribute('data-category');
      var show = value === 'all' || cat === value;
      if (show) {
        item.classList.remove('projects-hidden--collapsed');
        requestAnimationFrame(function () {
          item.classList.remove('projects-hidden');
        });
      } else {
        item.classList.add('projects-hidden');
        item.addEventListener('transitionend', function onEnd() {
          item.removeEventListener('transitionend', onEnd);
          item.classList.add('projects-hidden--collapsed');
        }, { once: true });
      }
    });
  }
  buttons.forEach(function (btn) {
    btn.addEventListener('click', function () {
      var value = btn.getAttribute('data-filter');
      setActive(btn);
      filterProjects(value);
    });
  });
})();
</script>
