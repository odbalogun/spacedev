<!-- Project detail modal -->
<div id="project-modal" class="project-modal hidden" role="dialog" aria-modal="true" aria-labelledby="project-modal-title" aria-hidden="true">
<div class="project-modal-backdrop" id="project-modal-backdrop" aria-hidden="true"></div>
<div class="project-modal-panel">
<button type="button" class="project-modal-close" id="project-modal-close" aria-label="Close project details">
<span class="material-symbols-outlined">close</span>
</button>
<div class="project-modal-content">
<div class="project-modal-gallery">
<div class="project-modal-image-wrap">
<img id="project-modal-image" class="project-modal-image" src="" alt=""/>
</div>
<button type="button" class="project-modal-gallery-prev" id="project-modal-gallery-prev" aria-label="Previous image"><span class="material-symbols-outlined">chevron_left</span></button>
<button type="button" class="project-modal-gallery-next" id="project-modal-gallery-next" aria-label="Next image"><span class="material-symbols-outlined">chevron_right</span></button>
<div class="project-modal-dots" id="project-modal-dots"></div>
</div>
<div class="project-modal-body">
<span id="project-modal-category" class="project-modal-category"></span>
<h2 id="project-modal-title" class="project-modal-title"></h2>
<p id="project-modal-location" class="project-modal-location"></p>
<div class="project-modal-stats" id="project-modal-stats"></div>
<h3 class="project-modal-overview-title">Overview</h3>
<p id="project-modal-overview" class="project-modal-overview"></p>
</div>
</div>
</div>
</div>
